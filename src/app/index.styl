@import 'jeet'
@import 'rupture'
@import url('http://fonts.googleapis.com/css?family=Lato:300,400,700,900&subset=latin,latin-ext')

.browsehappy
  margin 0.2em 0
  background #ccc
  color #000
  padding 0.2em 0

.clearfix:before,
.clearfix:after {
  content: "";
  display: table;
}
.clearfix:after {
  clear: both;
}
.clearfix {
  zoom: 1;
}

.fieldErrorMessages
  color red

body
  font-family Lato

